updater_version = "ALPHA 1 BUILD 3"\j\jimport requests\jimport re\j\jfrom modules import gitm\j\jdef update_updater():\j\kinfo3 = requests.get('https://github.com/Werryx/Console/blob/master/.updater_version')\j\koutput3 = "".join(re.findall(r"<td id=\"LC1\" class=\"blob-code blob-code-inner js-file-line\">(.*?)</td>", info3.text))\j\kif(updater_version != output3):\j\k\kfrom modules import updater3\j\k\kprint("Было найдено обновление \"" + output3 + "\" файла updater.py. Обновление уже установлено. Пожалуйста, перезапустите файл")\j\k\kexit()\jupdate_updater()\j\jprint("Поиск обновлений...")\j\jf = open(".version.txt")\jvernow = f.read()\jf.close()\j\jiv = requests.get('https://github.com/Werryx/Console/blob/master/.version.txt')\jver = "".join(re.findall(r"<td id=\"LC1\" class=\"blob-code blob-code-inner js-file-line\">(.*?)</td>", iv.text))\j\jdef update():\j\k# Обновление файла .data\j\kinfo = requests.get('https://github.com/Werryx/Console/blob/master/.data')\j\koutput = degit("".join(re.findall(r"<td id=\"LC1\" class=\"blob-code blob-code-inner js-file-line\">(.*?)</td>", info.text)))\j\kf = open(".data", "w")\j\kf.write(output)\j\kf.close()\j\j\k# Обновление файла .version.txt\j\kf = open(".version.txt", "w")\j\kf.write(ver)\j\kf.close()\j\kvernow = ver\j\j\k# Обновление файла main.py\j\kinfo2 = requests.get('https://github.com/Werryx/Console/blob/master/main.py.txt')\j\koutput2 = degit("".join(re.findall(r"<td id=\"LC1\" class=\"blob-code blob-code-inner js-file-line\">(.*?)</td>", info2.text)).replace("\j", "\n").replace("\k", "\t").replace("&quot;", "\""))\j\kf = open("main.py", "w")\j\kf.write(output2)\j\kf.close()\j\j\kprint("Файл обновлён до версии v." + vernow + " успешно")\j\jif(vernow != ver):\j\kupdate()\jelse:\j\kprint("Вы используете последнюю версию")
